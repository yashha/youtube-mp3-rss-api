{
  "name": "youtube-mp3-rss-api",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:ts-node": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "now --no-verify --public --docker",
    "deploy:start": "npm install && npm run start",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/microservices": "^5.4.0",
    "@nestjs/testing": "^5.4.0",
    "@nestjs/websockets": "^5.4.0",
    "filenamify": "^2.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rss": "^1.2.2",
    "rss-parser": "^3.5.3",
    "rxjs": "^6.2.2",
    "ytdl-core": "^0.28.0",
    "through2": "latest",
    "xtend": "latest"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/filenamify": "^2.0.1",
    "@types/fluent-ffmpeg": "^2.1.8",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/rss": "0.0.28",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
